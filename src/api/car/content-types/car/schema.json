{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "gasoline": {
      "type": "integer",
      "default": 70,
      "required": true
    },
    "steering": {
      "type": "string",
      "default": "Manual",
      "required": true
    },
    "capacity": {
      "type": "integer",
      "default": 2,
      "required": true
    },
    "price": {
      "type": "decimal",
      "default": 99,
      "required": true
    },
    "gallery": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
